  <Import Project="$([MSBuild]::GetDirectoryNameOfFileAbove($(MSBuildThisFileDirectory), dir.props))\buildConf\Generic.props" />

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Authors>$(DefaultAuthors), $(OtherAuthors)</Authors>
